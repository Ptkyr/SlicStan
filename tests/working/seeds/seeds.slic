data int I;
data int[I] n; // germinated seeds on each plate I
data int[I] N; // total number of seeds on each plate I
data vector[I] x1; // seed type of each plate I -- 0 or 1
data vector[I] x2; // type of root extract of each plate I -- 0 or 1

vector[I] x1x2 = x1 .* x2; // interaction

real sigma = 1.0 / sqrt(tau);

real alpha0 ~ normal(0.0,1000);
real alpha1 ~ normal(0.0,1000);
real alpha2 ~ normal(0.0,1000);
real alpha12 ~ normal(0.0,1000);
real tau ~ gamma(0.001,0.001);

vector[I] b ~ normal(0.0, sigma);
n ~ binomial_logit(N, alpha0 + alpha1 * x1 + alpha2 * x2 + alpha12 * x1x2 + b);