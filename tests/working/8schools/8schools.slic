def MyNormal(real m, real v){
  real xr ~ normal(0, 1); 
  return v * xr + m;
}

data real[8] y;
data real[8] sigma;

real mu;
real tau ~ gamma(1,1);
real[8] theta;

theta[1] = MyNormal(mu, tau);
theta[2] = MyNormal(mu, tau);
theta[3] = MyNormal(mu, tau);
theta[4] = MyNormal(mu, tau);
theta[5] = MyNormal(mu, tau);
theta[6] = MyNormal(mu, tau);
theta[7] = MyNormal(mu, tau);
theta[8] = MyNormal(mu, tau);
y ~ normal(theta, sigma);